{
	"server": {
		"host": "http://localhost",
		"portUrl": "PORT_API_URL",
		"portValue": ":3001",
		"readTimeoutS":10,
		"writeTimeoutS":15,
		"idleTimeoutS":5,
		"waitTimeoutS":0,
		"execTimeoutS":60
	},
	"dataBase": {
		"driverName": "postgres",
		"url": "DATABASE_URL",
		"connectionString": "postgres://rolepade:escapade@pg:5432/escabase?sslmode=disable",
		"maxOpenConns": 20,
		"PageGames": 2,
		"PageUsers": 100
	},
	"authClient": {
		"url": "AUTHSERVICE_URL",
		"address": ":3003"
	},
	"cors": {
		"origins": [
			"http://localhost:8080",
			"http://localhost:3000",
			"http://localhost:3001",
			"http://localhost:3002",
			"https://escapade-frontend.herokuapp.com",
			"https://ser.ru.com",
			"https://explosion.team"
		],
		"headers": [
			"Content-type"
		],
		"methods": [
			"GET", "POST", "DELETE", "PUT", "OPTIONS"
		],
		"credentials": "true"
	},
	"session": {
		"name": "sessionid",
		"path": "/",
		"length": 16,
		"lifetime": 604800,
		"httpOnly": true
	}
}
