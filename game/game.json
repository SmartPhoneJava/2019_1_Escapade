{
	"server": {
		"host": "http://localhost",
		"portUrl": "PORT_GAME_URL",
		"portValue": ":3002"
	},
	"authClient": {
		"url": "AUTHSERVICE_URL",
		"adress": ":3003"
	},
	"dataBase": {
		"driverName": "postgres",
		"url": "DATABASE_URL",
		"connectionString": "dbname=escabase user=rolepade password=escapade sslmode=disable",
		"maxOpenConns": 20
	},
	"cors": {
		"origins": [
			"http://localhost:8080",
			"http://localhost:3001",
			"http://localhost:3002",
			"https://escapade-frontend.herokuapp.com"
		],
		"headers": [
			"Content-type"
		],
		"credentials": "true",
		"methods": [
			"GET",
			"POST",
			"PUT",
			"DELETE",
			"OPTIONS"
		]
	},
	"session": {
		"name": "sessionid",
		"path": "/",
		"length": 16,
		"lifetime": 604800,
		"httpOnly": true
	},
	"game": {
		"roomsCapacity": 500,
		"lobbyJoin": 1,
		"lobbyRequest": 1
	},
	"websocket": {
		"writeWait": 60,
		"pongWait": 10,
		"pingPeriod": 9,
		"maxMessageSize": 512,
		"readBufferSize": 1024,
		"writeBufferSize": 1024
	}
}